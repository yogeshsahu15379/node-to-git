{
  "name": "@aes-digital/aes-design",
  "description": "AES Design System",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/aesds/aesds.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "loader",
    "tailwind.config.js"
  ],
  "scripts": {
    "generate.resolved-tailwind": "node scripts/resolve-tailwind-config.js",
    "generate.custom-elements": "wca analyze 'src/components/*/*.tsx' --format json --outFile .storybook/custom-elements.json",
    "prebuild": "npm run generate.resolved-tailwind",
    "build": "NODE_ENV=production stencil build --docs",
    "build-analyze": "NODE_ENV=production STENCIL_ANALYZE=true stencil build --docs",
    "prebuild.watch": "npm run generate.resolved-tailwind",
    "build.watch": "stencil build --watch",
    "prestart": "npm run generate.resolved-tailwind",
    "start": "stencil build --dev --watch --serve",
    "dev": "npm run storybook.dev.watch",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "lint": "eslint . && prettier --check .",
    "fmt": "concurrently \"npm:fmt.code\" \"npm:fmt.svg\"",
    "fmt.code": "eslint --fix . && prettier --write .",
    "fmt.svg": "svgo -f ./src -r --config=.svgo.yml",
    "storybook.wait": "wait-on dist/esm/loader.js .storybook/custom-elements.json src/resolved-tailwind-config.ts",
    "storybook.dev": "npm run storybook.wait && start-storybook -s ./.storybook/static -p 6007",
    "storybook.dev.watch": "concurrently -c \"cyan,blue,magenta\" \"npm:generate.custom-elements\" \"npm:build.watch\" \"npm:storybook.dev\"",
    "storybook.prod": "npm run storybook.wait && NODE_ENV=production build-storybook -o storybook-static",
    "storybook.prod.build": "concurrently -c \"cyan,blue,magenta\" \"npm:generate.custom-elements\" \"npm:build\" \"npm:storybook.prod\"",
    "semantic-release": "semantic-release",
    "debug-release": "semantic-release -d --debug"
  },
  "dependencies": {
    "@stencil/core": "2.3.0",
    "jarallax": "1.12.5",
    "keen-slider": "5.4.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/gitlab": "^6.2.2",
    "@semantic-release/npm": "7.0.10",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@stencil/eslint-plugin": "0.3.1",
    "@stencil/postcss": "2.0.0",
    "@storybook/addon-a11y": "6.1.20",
    "@storybook/addon-actions": "6.1.20",
    "@storybook/addon-essentials": "6.1.20",
    "@storybook/addon-links": "6.1.20",
    "@storybook/addon-storysource": "6.1.20",
    "@storybook/web-components": "6.1.20",
    "@types/jest": "26.0.20",
    "@types/puppeteer": "5.4.2",
    "@types/responselike": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "4.14.0",
    "@typescript-eslint/parser": "4.14.0",
    "classnames": "2.2.6",
    "concurrently": "5.3.0",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-react": "7.22.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "jest-cli": "26.6.3",
    "lint-staged": "10.5.3",
    "lit-html": "1.3.0",
    "postcss": "8.2.4",
    "postcss-import": "14.0.0",
    "postcss-loader": "4.2.0",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.2.1",
    "puppeteer": "5.5.0",
    "puppeteer-core": "5.5.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "semantic-release": "^17.4.0",
    "svgo": "1.3.2",
    "tailwindcss": "1.9.6",
    "wait-on": "5.2.1",
    "web-component-analyzer": "1.1.6"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:aes-digital/global-communication/drupal/aes-design.git"
  },
  "author": "",
  "version": "1.0.0"
}
